          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="User Name" textFill="#5b0101" GridPane.rowIndex="1">
               <font>
                  <Font name="Abyssinica SIL" size="16.0" />
               </font>
            </Label>
            <Label text="Password" textFill="#5b0101" GridPane.rowIndex="2">
               <font>
                  <Font name="Abyssinica SIL" size="16.0" />
               </font>
            </Label>
            <CheckBox fx:id="isManagerCheckBox" mnemonicParsing="false" text="Login as Manager" textFill="#2a018a" GridPane.columnSpan="2" GridPane.rowIndex="3">
